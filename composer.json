{
    "name": "genealabs/laravel-collection-macros",
    "description": "Extending Laravel Collections with useful functionality.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "illuminate/pagination": "5.6 - 6.0",
        "illuminate/support": "5.6 - 6.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "*",
        "fzaninotto/faker": "*",
        "mockery/mockery": "*",
        "orchestra/testbench-browser-kit": "3.9.x-dev@dev",
        "orchestra/testbench-dusk": "3.9.x-dev@dev",
        "orchestra/testbench": "3.9.x-dev@dev",
        "php-coveralls/php-coveralls" : "*",
        "phpmd/phpmd": "*",
        "phpunit/phpunit": "*",
        "sebastian/phpcpd": "*",
        "symfony/thanks": "*"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelCollectionMacros\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeneaLabs\\LaravelCollectionMacros\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ],
            "providers": [
                "GeneaLabs\\LaravelCollectionMacros\\Providers\\Service"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
